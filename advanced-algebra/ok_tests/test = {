test = {
    'name': 'digits-test',
    'suites': [
        {
            'cases': [
                {
                    'code': r"""
                    >>> # This is a doctest!
                    >>> len(digits) == 9
                    True
                    """
                },
                {
                    'code': r"""
                    >>> # This is another doctest.
                    >>> # This will run if the previous doctest passed
                    >>> digits = [1,2,3,4,5,6,7,8,9]
                    True
                    """
                }
            ]
        }
    ]
}